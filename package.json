{
  "name": "screeps-typescript",
  "version": "3.0.0",
  "description": "Screeps automation code",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "lint-stats": "eslint \"src/**/*.ts\" --ext .js,.ts --format \"node_modules/eslint-stats/byErrorAndWarning.js\"",
    "build": "rollup -c",
    "deploy-prod": "cross-env NODE_ENV=production rollup -c",
    "deploy-dev": "cross-env NODE_ENV=development rollup -c",
    "push-main": "cross-env NODE_ENV=production rollup -c --environment DEST:main",
    "push-season": "cross-env NODE_ENV=production rollup -c --environment DEST:season",
    "push-pserver": "cross-env NODE_ENV=production rollup -c --environment DEST:pserver",
    "push-sim": "cross-env NODE_ENV=development rollup -c --environment DEST:sim",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-cov": "jest --coverage",
    "test-only": "bash -c 'jest --watch --coverage --verbose --collectCoverageFrom=\"**/*$1*\" $1' -- ",
    "test-integration": "echo 'See docs/in-depth/testing.md for instructions on enabling integration tests'",
    "watch-main": "cross-env NODE_ENV=production rollup -cw --environment DEST:main",
    "watch-season": "cross-env NODE_ENV=production rollup -cw --environment DEST:season",
    "watch-pserver": "cross-env NODE_ENV=production rollup -cw --environment DEST:pserver",
    "watch-sim": "cross-env NODE_ENV=development rollup -cw --environment DEST:sim"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mortenbroesby/screeps-typescript.git"
  },
  "author": "Morten Broesby-Olsen",
  "license": "Unlicensed",
  "homepage": "https://github.com/mortenbroesby/screeps-typescriptr#readme",
  "engines": {
    "node": "10.x || 12.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/chai": "^4.1.6",
    "@types/jest": "^24.0.15",
    "@types/lodash": "3.10.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^13.13.1",
    "@types/screeps": "^3.1.0",
    "@types/sinon": "^5.0.5",
    "@types/sinon-chai": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "@typescript-eslint/typescript-estree": "^3.7.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-stats": "^1.0.1",
    "husky": "^4.3.8",
    "jest": "^24.8.0",
    "lodash": "^3.10.1",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-nodent": "^0.2.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-screeps": "^1.0.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-uglify": "^6.0.4",
    "screeps-jest": "eduter/screeps-jest#1.2.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "dayjs": "^1.10.4",
    "screeps-lru-cache": "2.1.10",
    "source-map": "~0.6.1"
  }
}
