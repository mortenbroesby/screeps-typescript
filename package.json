{
  "name": "screeps-typescript",
  "version": "3.1.7",
  "description": "Screeps automation code",
  "main": "index.js",
  "scripts": {
    "type-check": "tsc --project ./src/tsconfig.json --noEmit",
    "tsc": "tsc --project ./src/tsconfig.build.json --module commonjs --outDir lib",
    "lint": "eslint . --ext .js,.ts",
    "lint-stats": "eslint . --ext .js,.ts --format \"node_modules/eslint-stats/byErrorAndWarning.js\"",
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean",
    "build": "node build/build.js",
    "deploy": "node build/deploy.js",
    "deploy-prod": "npm run deploy --environment=\"production\"",
    "deploy-dev": "npm run deploy --environment=\"development\"",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-cov": "jest --coverage",
    "test-only": "bash -c 'jest --watch --coverage --verbose --collectCoverageFrom=\"**/*$1*\" $1' -- "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mortenbroesby/screeps-typescript.git"
  },
  "author": "Morten Broesby-Olsen",
  "license": "Unlicensed",
  "homepage": "https://github.com/mortenbroesby/screeps-typescriptr#readme",
  "engines": {
    "node": "10.x || 12.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/chai": "^4.1.6",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^5.2.5",
    "@types/node": "^13.13.1",
    "@types/screeps": "^3.1.0",
    "@types/sinon-chai": "^3.2.0",
    "@types/sinon": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "@typescript-eslint/typescript-estree": "^3.7.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-stats": "^1.0.1",
    "eslint": "^6.8.0",
    "husky": "^4.3.8",
    "jest": "^24.8.0",
    "lodash": "^4.17.21",
    "npm-run-all": "4.1.5",
    "ora": "^5.3.0",
    "prettier": "^2.0.4",
    "screeps-jest": "eduter/screeps-jest#1.2.0",
    "screeps-modules": "^1.2.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "webpack-cli": "^4.5.0",
    "webpack": "^5.24.3",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "core-js": "^3.9.1",
    "dayjs": "^1.10.4",
    "screeps-lru-cache": "2.1.10",
    "source-map": "~0.6.1"
  }
}
